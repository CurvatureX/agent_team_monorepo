# Supabase配置 (只需要SECRET_KEY)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SECRET_KEY=your-service-key

# AI模型配置
OPENAI_API_KEY=your-openai-api-key
DEFAULT_MODEL_PROVIDER=openai
DEFAULT_MODEL_NAME=gpt-4o-mini

# 测试账号（用于集成测试）
TEST_USER_EMAIL=your-test-email@example.com
TEST_USER_PASSWORD=your-test-password

# 服务配置
API_GATEWAY_PORT=8000
WORKFLOW_AGENT_PORT=50051

# 环境配置
ENVIRONMENT=development
DEBUG=true

# Slack configuration
DEFAULT_SLACK_BOT_TOKEN=your-slack-bot-token

# Notion OAuth configuration
NOTION_CLIENT_ID=your-notion-client-id
NOTION_CLIENT_SECRET=your-notion-client-secret
NOTION_REDIRECT_URI=https://your-domain.com/api/v1/public/webhooks/notion/auth
NOTION_ACCESS_TOKEN=your-notion-access-token
